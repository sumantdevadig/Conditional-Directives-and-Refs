<apex:page controller="InsertContact">
  <apex:form >
     <apex:pageBlock >
        <apex:pageBlockSection columns="3" title="Contact Information">
            <apex:inputText value="{!InsertContact.FirstName}"/>
            <apex:inputText value="{!InsertContact.LastName}"/>
            <apex:inputText value="{!InsertContact.Title}"/>
            <apex:inputText value="{!InsertContact.Email}"/>
        </apex:pageBlockSection>
        <apex:pageBlockButtons >
            <apex:commandButton value="Save" action="{!save}"/>
        </apex:pageBlockButtons>
     </apex:pageBlock>
  </apex:form>

</apex:page>